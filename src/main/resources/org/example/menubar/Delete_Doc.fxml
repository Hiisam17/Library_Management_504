<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.menubar.DeleteDocumentController" spacing="10" alignment="CENTER">
    <Label text="Nhập ID của tài liệu cần xóa:"/>
    <TextField fx:id="deleteIdField" promptText="ID tài liệu"/>
    <Button text="Xác nhận Xóa" onAction="#handleConfirmDelete"/>
    <Button text="Hủy" onAction="#handleCancel"/>
</VBox>

